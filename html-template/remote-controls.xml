<?xml version="1.0"?>
<remotecontrols>
	<remoteobjects />
	<httpservices>
		<httpservice>
			<name>portfoliosList</name>
			<url>/portfolios_sam_list/</url>
			<resultformat>json</resultformat>
		</httpservice>
		<httpservice>
			<name>portfolioPerformances</name>
			<url>/portfolio_performances/</url>
			<post>
				<arg>start_date</arg>
				<arg>end_date</arg>
				<arg>portfolio_id</arg>
			</post>
			<resultformat>json</resultformat>
		</httpservice>
		<httpservice>
			<name>portfolioBreakdown</name>
			<url>/portfolio_breakdown/</url>
			<post>
				<arg>portfolio_id</arg>
				<arg>query_date</arg>
				<arg>breakdown</arg>
			</post>
			<resultformat>json</resultformat>
		</httpservice>
		<httpservice>
			<name>indicesList</name>
			<url>/indices_list/</url>
			<resultformat>json</resultformat>
		</httpservice>
		<httpservice>
			<name>securityPerformances</name>
			<url>/security_performances/</url>
			<post>
				<arg>start_date</arg>
				<arg>end_date</arg>
				<arg>security_id</arg>
			</post>
			<resultformat>json</resultformat>
		</httpservice>
		<httpservice>
			<name>createPortfolioAnnotation</name>
			<url>/create_portfolio_annotation/</url>
			<post>
				<arg>annotations</arg>
			</post>
			<resultformat>json</resultformat>
		</httpservice>
		<httpservice>
			<name>portfolioAnnotation</name>
			<url>/portfolio_annotation/</url>
			<post>
				<arg>portfolio_id</arg>
				<arg>query_date</arg>
				<arg>target</arg>
			</post>
			<resultformat>json</resultformat>
		</httpservice>
	</httpservices>
</remotecontrols>